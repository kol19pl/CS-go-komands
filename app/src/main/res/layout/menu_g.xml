<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#68686B"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/ad_view_container_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </FrameLayout>

    <ScrollView
        android:id="@+id/scrollView8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toTopOf="@+id/ad_view_container_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/button4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Bronie"
                android:text="@string/komendy_na_bronie"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Granaty"
                android:text="@string/komendy_na_granaty_dwa"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Boty"
                android:text="@string/boty"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Celownik"
                android:text="@string/komendy_na_celownik"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Radar"
                android:text="@string/radar"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="KomendydoCwiczen"
                android:text="@string/komendy_do_wicze"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="FPS"
                android:text="@string/fps"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Hud"
                android:text="@string/hud"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Myszka"
                android:text="@string/myszka"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Dzwienk"
                android:text="@string/d_wi_k"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="PozycjaBroni"
                android:text="@string/pozycja_broni"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="ZarzadzanieSerwerem"
                android:text="@string/zarz_dzanie_serwerem"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Loncze"
                android:text="@string/cze"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Demo"
                android:text="@string/nagrywanie_obs_uga_dema"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Bindy"
                android:text="@string/bindy"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button26"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="Oznaczenia"
                android:text="@string/oznaczenia"
                android:background="@drawable/buton"
                android:textColor="@android:color/background_light"/>

            <Button
                android:id="@+id/button16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="Inne"
                android:text="@string/inne"
                android:textColor="@android:color/background_light" />

            <Button
                android:id="@+id/button17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:onClick="ParametryStartowe"
                android:text="@string/parametry_startowe"
                android:textColor="@android:color/background_light" />

            <TextView
                android:id="@+id/textView61"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/buton"
                android:textColor="@android:color/background_light" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>